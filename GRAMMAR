start:

bang: BANG ACCOUNT accountDetails
bang: BANG CLEAR COLON AUTOSWITCH
bang: BANG OPTION COLON AUTOSWITCH
bang: BANG TYPE COLON BANK bankDetails
bang: BANG TYPE COLON CATEGORY categoryDetails 
bang: BANG TYPE COLON CCARD creditCardDetails
bang: BANG TYPE COLON INVEST investmentDetails
bang: BANG TYPE COLON MEMORIZED memorizedTransactionDetails
bang: BANG TYPE COLON PRICES priceDetails
bang: BANG TYPE COLON TAG tagDetails
bang: BANG TYPE COLON SECURITY securityDetails

accountSection: "!Account" EOL account
accountDetails: accountDetail+

accountDetail: ((statementBalanceDate | statementBalance | description | creditLimit | name | accountType) EOL)+ CARET EOL

categorySection: "!Type:Cat" EOL categoryDetail*
categoryDetail: (
    categoryBudgetAmount |
    categoryDescription | 
    categoryExpenseFlag |
    categoryIncomeFlag
    categoryName | 
    categoryTaxRelated | 
    categoryTaxSchedule )* categoryEndOfRecord
categoryName: "N" TEXT EOL
categoryDescription: "D" TEXT EOL
categoryTaxRelated: "T" EOL
categoryTaxSchedule: "R" TEXT EOL
categoryIncomeFlag: "I" EOL
categoryExpenseFlag: "E" EOL
categoryBudgetAmount: "B" AMOUNT EOL

securityDetails: securityDetail+
securityDetail: G RISK
securityDetail: N LABEL
securityDetail: S SYMBOL
securityDetail: T TYPE

tagSection: "!Type:Tag" EOL tagDetail*
tagDetail: (tagName | tagDescription)* tagEndRecord
tagName: "N" TEXT EOL
tagDescription: "D" TEXT EOL
tagEndRecord: "^" EOL

// " 2/ 2'18"
// "11/22'16"
date: QUOTE ((SPACE DIGIT) | (DIGIT DIGIT)) SLASH ((SPACE DIGIT) | (DIGIT DIGIT)) TIC DIGIT DIGIT

detailDate: (DIGIT DIGIT?) SLASH (SPACE | DIGIT) DIGIT TIC DIGIT DIGIT